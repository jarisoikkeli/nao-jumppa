                <content>
                  <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        pass

    def onUnload(self):
        pass

    def onInput_onStart(self):
        import time
        from naoqi import ALProxy

        # Proxy yhteydet
        tts = ALProxy("ALTextToSpeech", "127.0.0.1", 9559)
        motion = ALProxy("ALMotion", "127.0.0.1", 9559)

        # Herätetään robotti
        motion.wakeUp()

        # Viisi toistoa
        for numero in ["Yksi", "Kaksi", "Kolme", "Neljä", "Viisi"]:
            # Nostetaan kädet sivulle ylös
            names  = ["LShoulderRoll", "RShoulderRoll"]
            angles = [1.3, -1.3]  # kädet ylös sivuille
            fractionMaxSpeed = 0.1
            motion.setAngles(names, angles, fractionMaxSpeed)

            time.sleep(3)

            # Sanotaan numero
            tts.say(numero)

            # Lasketaan kädet alas
            angles = [0.0, 0.0]  # kädet alas vartalon viereen
            motion.setAngles(names, angles, fractionMaxSpeed)

            time.sleep(3)

        # Lopetusviesti
        tts.say("Hyvä! Reipasta toimintaa.")

        self.onStopped()  # aktivoi boksin outputin

    def onInput_onStop(self):
        self.onUnload()
        self.onStopped()]]>
