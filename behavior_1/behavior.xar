              <script language="4">
                <content>
                  <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        pass

    def onUnload(self):
        pass

    def onInput_onStart(self):
        import time

        # Käytetään TTS- ja Motion-proxya
        from naoqi import ALProxy
        tts = ALProxy("ALTextToSpeech", "127.0.0.1", 9559)
        motion = ALProxy("ALMotion", "127.0.0.1", 9559)

        # Varmistetaan että robotti on hereillä
        motion.wakeUp()

        # Alkuviesti
        tts.say("Tehdään käsien ja olkapään liikkuvuus jumppa.")

        # Viisi toistoa
        for i, numero in enumerate(["Yksi", "Kaksi", "Kolme", "Neljä", "Viisi"], start=1):
            # Nostetaan kädet ylös
            names  = ["LShoulderPitch", "RShoulderPitch"]
            angles = [-1.0, -1.0]  # kädet ylös
            fractionMaxSpeed = 0.1
            motion.setAngles(names, angles, fractionMaxSpeed)

            time.sleep(3)  # odotetaan että kädet nousevat hitaasti

            # Sanotaan numero
            tts.say(numero)

            # Lasketaan kädet alas
            angles = [1.5, 1.5]  # kädet alas
            motion.setAngles(names, angles, fractionMaxSpeed)

            time.sleep(3)

        # Lopetus
        tts.say("Hyvä! Sillälailla!")

        self.onStopped()  # Aktivoi boksin output

    def onInput_onStop(self):
        self.onUnload()
        self.onStopped()]]>
