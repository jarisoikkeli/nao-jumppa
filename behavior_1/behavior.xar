              <script language="4">
                <content>
                  <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        pass

    def onUnload(self):
        pass

    def onInput_onStart(self):
        import time
        from naoqi import ALProxy

        # Proxy-yhteydet
        tts = ALProxy("ALTextToSpeech", "127.0.0.1", 9559)
        motion = ALProxy("ALMotion", "127.0.0.1", 9559)

        # Herätetään robotti
        motion.wakeUp()

        # Viisi toistoa
        for numero in ["Yksi", "Kaksi", "Kolme", "Neljä", "Viisi"]:
            # --- Etukautta ylös ---
            names  = ["LShoulderPitch", "RShoulderPitch"]
            angles = [-1.0, -1.0]   # kädet etukautta ylös
            motion.setAngles(names, angles, 0.1)
            time.sleep(3)

            # --- Etukautta alas ---
            angles = [1.5, 1.5]    # kädet alas
            motion.setAngles(names, angles, 0.1)
            time.sleep(3)

            # --- Sivulle ylös ---
            names  = ["LShoulderRoll", "RShoulderRoll"]
            angles = [1.3, -1.3]   # kädet sivulle ylös
            motion.setAngles(names, angles, 0.1)
            time.sleep(3)

            # --- Sivulle alas ---
            angles = [0.0, 0.0]    # alas vartalon viereen
            motion.setAngles(names, angles, 0.1)
            time.sleep(3)

            # Lasketaan ääneen
            tts.say(numero)

        # Lopetusviesti
        tts.say("Hyvä! Erinomaista liikkuvuutta.")

        # Rentoutetaan nivelet
        motion.rest()

        self.onStopped()

    def onInput_onStop(self):
        self.onUnload()
        self.onStopped()]]>
